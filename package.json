{
	"name": "zendesk_react_app_boilerplate",
	"version": "2.0.0",
	"description": "Boilerplate for making a Zendesk App with React.js and Redux ",
	"scripts": {
		"clean": "rimraf dist/tmp",
		"build": "npm run lint && webpack",
		"profile": "webpack --profile --json > webpack-stats.json",
		"build-dev": "webpack --config webpack.dev.js",
		"package": "npm-run-all clean && zat package --path ./dist",
		"build:package": "npm-run-all build package",
		"dev": "webpack serve --hot",
		"serve": "zat server --path dist -c settings.json",
		"lint": "eslint src/",
		"test": "jest"
	},
	"author": "Cloudhuset.dk",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.1",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.6.1",
		"babel-loader": "^8.1.0",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^5.0.0",
		"eslint": "^7.12.1",
		"eslint-import-resolver-webpack": "^0.13.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-react": "^7.21.5",
		"eslint-watch": "^7.0.0",
		"html-loader": "^1.3.2",
		"html-webpack-plugin": "^5.0.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^26.6.1",
		"node-sass": "^4.14.1",
		"npm-run-all": "^4.1.5",
		"react-refresh": "^0.9.0",
		"rimraf": "^3.0.2",
		"sass-loader": "^10.0.4",
		"style-loader": "^2.0.0",
		"svg-inline-loader": "^0.8.2",
		"terser-webpack-plugin": "^5.0.1",
		"webpack": "^5.21.2",
		"webpack-cli": "4.5.0",
		"webpack-dev-server": "3.11.2",
		"webpack-merge": "5.7.3"
	},
	"dependencies": {
		"@babel/runtime": "^7.12.1",
		"core-js": "^3.6.5",
		"promise-polyfill": "^8.2.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"regenerator-runtime": "^0.13.7"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
			"\\.(css|less|scss)$": "identity-obj-proxy"
		}
	}
}
